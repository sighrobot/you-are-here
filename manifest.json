{
    "name": "You Are Here",
    "version": "1.0.1",
    "description": "Find out exactly how you got to any web page.",
    "permissions": [
      "history",
      "tabs",
      "chrome://favicon/",
      "webNavigation"
    ],
    "content_security_policy": "script-src 'self' https://cdnjs.cloudflare.com/ajax/libs/cytoscape/3.8.2/cytoscape.min.js; object-src 'self'; img-src chrome://favicon;",
    "background": {
      "scripts": [
        "handlers.js",
        "utils.js",
        "background.js"
      ],
      "persistent": true
    },
    "browser_action": {
      "default_popup": "popup/index.html"
    },
    "icons": {
     "128": "favicon.png"
    },
    "manifest_version": 2
  }
